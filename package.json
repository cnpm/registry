{
  "name": "cnpm-registry",
  "version": "0.0.0",
  "description": "Private npm registry for Enterprise, base on eggjs",
  "dependencies": {
    "egg": "^2.11.2",
    "egg-parameters": "^0.1.0",
    "egg-scripts": "^2.9.1",
    "egg-sequelize": "^4.1.0",
    "egg-validate": "^2.0.1",
    "mysql2": "^1.6.1",
    "utility": "^1.15.0",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.9.0",
    "egg-ci": "^1.9.2",
    "egg-mock": "^3.20.1",
    "eslint": "^5.6.1",
    "eslint-config-egg": "^7.1.0",
    "sequelize-cli": "^4.1.1",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=10.11.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon",
    "stop": "egg-scripts stop",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run create_sql && npm run test-local",
    "create_sql": "node scripts/create_sql.js",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run create_sql && npm run cov",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all",
    "init-database": "mysql -uroot -e 'CREATE DATABASE IF NOT EXISTS `cnpm_registry_unittest`;'",
    "init-and-ci": "npm run init-database && NODE_ENV=test npm run db:migrate && npm run ci",
    "autod": "autod"
  },
  "ci": {
    "type": "travis",
    "services": "mysql",
    "version": "10",
    "os": {
      "travis": "linux"
    },
    "command": {
      "travis": "init-and-ci"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cnpm/registry.cnpmjs.org.git"
  },
  "license": "MIT"
}
